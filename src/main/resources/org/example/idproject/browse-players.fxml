<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.idproject.view.PlayerBrowsingController">
   <children>
      <VBox alignment="CENTER" layoutX="95.0" layoutY="66.0" prefHeight="280.0" prefWidth="424.0" spacing="20.0">
         <children>
            <TextField alignment="TOP_CENTER" promptText="Search player by nickname" prefHeight="15.0" prefWidth="75.0" />
             <Button text="Search" onAction="#handleSearch"/>
            <TableView prefHeight="231.0" prefWidth="424.0">
              <columns>
                <TableColumn prefWidth="91.0" text="ID" />
                <TableColumn prefWidth="343.0" text="Current nickname" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
